// 1 - 1
// 3 - 1
// 5 - 1
// 7 - 1
// 9 - 1
#include <stdio.h>

int main() {
    
    int arr[]={1,2,3,4,5,6,7,8,9};
    int size  =  sizeof(arr)/sizeof(arr[0]);
    int count[10]={0};
    
    for(int i = 0; i < size; i++){
        count[arr[i]]++;
    }
    
    printf("frequency odd number: ");
    for(int  i = 0; i < size; i++){
        if(count[arr[i]]==1 && arr[i]%2==1){
            printf("\n%d - %d",arr[i], count[arr[i]]);
            count[arr[i]]=0;
        }
    }

    return 0;
}