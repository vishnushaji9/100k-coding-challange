#include <stdio.h>

int main() {
    
    int arr[]={1,7,2,5,3,4,7,1,9};
    int size = sizeof(arr)/sizeof(arr[0]);
    int i,j;
    
    for(i = 0; i <  size; i++){
        int isprime = 1;
        if(arr[i] < 2){
           isprime = 0; 
        }else{
            for(j = 2; j <= arr[i]/2; j++){
                if(arr[i]%j==0){
                isprime = 0;
                break;
                }
            }
        }
        if(isprime==0){
            for(int k = i ; k < size; k++){
                arr[k] = arr[k+1];
            }
            size--;
            i--;
        }
    }
    for(i = 0; i < size; i++){
      printf("%d ",arr[i]);  
    }

    return 0;
}



