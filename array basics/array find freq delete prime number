#include <stdio.h>

int main(){
    
    int arr[]={1,2,3,4,5,6,5,4,5,7,4,8,9};
    int size = sizeof(arr)/sizeof(arr[0]);
    int i,j;
    int count[20]={0};
    
    for(i = 0; i < size; i++){
        count[arr[i]]++;
    }
    
    for(i = 0; i < size; i++){
        int isprime = 1;
        if(arr[i]<2){
            isprime = 0;
        }else{
            for(j = 2; j <= arr[i]/2; j++){
                if(arr[i]%j==0){
                    isprime=0;
                    break;
                }
            }
        }
        if(isprime==0){
            for(int k = i; k < size; k++){
                arr[k] = arr[k + 1];
            }
            size--;
            i--;
        }
    }
    for(i = 0; i < size; i++){
        if(count[arr[i]]!=0){
            printf("\n%d - %d",arr[i] ,count[arr[i]]);
            count[arr[i]]=0;
        }
    }
    return 0;
}