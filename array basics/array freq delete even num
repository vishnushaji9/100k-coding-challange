// frequency odd number: 
// 2 - 1
// 4 - 1
// 6 - 1
// 8 - 1
#include <stdio.h>

int main() {
    
    int arr[]={1,2,3,4,5,6,7,8,9};
    int size  =  sizeof(arr)/sizeof(arr[0]);
    int count[10]={0};
    
    for(int i = 0; i < size; i++){
        count[arr[i]]++;
    }
    
    printf("element delete in odd number: ");
    for(int  i = 0; i < size; i++){
        if(count[arr[i]]==1 && arr[i]%2==0){
            for(int  j = i; j < size; j++){
                arr[j] = arr[j + 1];
            }
            size--;
            i--;
        }
    }
    for(int i = 0; i < size; i++){
        printf("\n%d - %d",arr[i],count[arr[i]]);
    }

    return 0;
}